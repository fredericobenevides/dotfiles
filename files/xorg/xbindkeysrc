# Bind "back" mouse button to meta+left
# "xte 'keydown Super_L' 'key 113' 'keyup Super_L'"
#   b:9

"xte 'keydown Control_L' 'keydown Alt_L' 'key Left' 'keyup Control_L' 'keyup Alt_L'"
  b:9

# Bind "forward" mouse button to Ctrl+F9
# "xte 'keydown Super_L' 'key 114' 'keyup Super_L'"
  # b:8

"xte 'keydown Control_L' 'keydown Alt_L' 'key Right' 'keyup Control_L' 'keyup Alt_L'"
  b:8
